{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "js/bootstrap.bundle.min.js",
        "js/bootstrap.bundle.min.js.map",
        "scss/**"
      ]
    },
    {
      "library": "bootstrap-icons@1.11.3",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "aspnet-client-validation@0.11.0",
      "destination": "wwwroot/lib/aspnet-client-validation/",
      "files": [
        "dist/aspnet-validation.min.js",
        "dist/aspnet-validation.min.js.map"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "highlight.js@11.8.0",
      "destination": "wwwroot/lib/highlight.js/",
      "files": [
        "highlight.min.js",
        "styles/**",
        "languages/**"
      ]
    }
  ]
}